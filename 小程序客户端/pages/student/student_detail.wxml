<view class="container">
  <loading hidden="{{loadingHide}}">
    {{loadingText}}
  </loading>

  <view class="title">学生信息详情</view>
  <view class="student_detail_view"> 
      <view class="viewItem">
        <view class="label">学号：</view>
        <view class="content">
          <view>{{studentNumber}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">学生密码：</view>
        <view class="content"> 
          <view>{{password}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">所在班级：</view>
        <view class="content"> 
          <view>{{classInfoId}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">姓名：</view>
        <view class="content"> 
          <view>{{studentName}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">性别：</view>
        <view class="content"> 
          <view>{{sex}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">学生照片：</view>
        <view class="content"> 
          <image mode="aspectFill" src="{{photoUrlUrl}}"></image>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">出生日期：</view>
        <view class="content"> 
          <view>{{birthday}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">政治面貌：</view>
        <view class="content"> 
          <view>{{zzmm}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">联系电话：</view>
        <view class="content"> 
          <view>{{telephone}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">家庭地址：</view>
        <view class="content"> 
          <view>{{address}}</view>
        </view>
      </view>

      <view>
        <navigator class="btnReturn" open-type="navigateBack" hover-class="navigator-hover">返回</navigator>
      </view>

  </view>
</view>
